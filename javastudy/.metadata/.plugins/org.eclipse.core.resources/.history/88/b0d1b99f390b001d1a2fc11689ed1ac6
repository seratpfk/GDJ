package study01;

public class HelloWorld {

	public static void main(String[] args) {
		
		int score = 100;
		char grade;
		
		if(score < 0 || score > 100) {
			grade = 'X';
	    } else if(score >= 90) {
			grade = 'A';
		} else if(score >= 80) {
			grade = 'B';
		} else if(score >= 70) {
			grade = 'C';
		} else if(score >= 60) {	
			grade = 'D';
		} else {
			grade = 'F';
		}
		
		System.out.println("점수는 " + score + "점");
		System.out.println("학점은 " + grade + "학점");
		
			
		
		
	}

}
