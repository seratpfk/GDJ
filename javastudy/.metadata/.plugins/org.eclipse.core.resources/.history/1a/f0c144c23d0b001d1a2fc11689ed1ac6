package study01;

public class HelloWorld {

	public static void main(String[] args) {
		
		int month = 11;
		
		switch((month-1)/3) {
		case 0: System.out.println("1분기"); break;
		case 1: System.out.println("2분기"); break;
		case 2: System.out.println("3분기"); break;
		default: System.out.println("4분기"); 
		}
		
		
		
			
		
		
	}

}
