package study01;

public class HelloWorld {

	public static void main(String[] args) {
		
		int balance = 79350;
		int money = 450;
		
		while(balance >= money) {
			System.out.println("잔액 " + balance + "인출액 " + money);
			balance -= money;
		}
		System.out.println("잔액 " + balance);
		
		
		
		
		
		}
	
	
		
			
		
		
	}


